<script>

  function getSubcatData(){
    var catData = document.getElementById('categoryID').value;
    $.ajax({
      url : "/admin/extra_category/getSubcat",
      method : "post",
      data : {
        'catid' : catData,
      },
      success : function(res){
        $('#SubCatId').html(res)
      }
    })
  }

  function getextraData(){
    var SubCatData = document.getElementById('SubCatId').value;
    $.ajax({
      url : "/admin/extra_category/getExtra",
      method : "post",
      data : {
        'subid' : SubCatData,
      },
      success : function(res){
        $('#extraId').html(res);
      }
    })
  }

  function getbrandId(){
        var catId = document.getElementById("categoryID").value;
        var subId = document.getElementById("SubCatId").value;
        var extraId = document.getElementById("extraId").value;
        console.log(catId);
        console.log(subId);
        console.log(extraId);
        $.ajax({
            url:"/admin/extra_category/getBrandTypeData",
            type:"post",
            data:{
                categoryId : catId,
                subCatId : subId,
                extraCatId : extraId
            },
            success:function(r){
                $("#brandTypeId").html(r);
                // alert("hi");
            }
        })
    }
</script>


