<%- include("header")%>


<main id="main" class="main">

    <div class="pagetitle">
      <h1>Add Brand Data</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/admin/deshboard">Home</a></li>
          <li class="breadcrumb-item">Brand</li>
          <li class="breadcrumb-item active">Add Brand</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-10">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Brand Form</h5>
              <!-- General Form Elements -->
              <form action="/admin/brand/add_BrandData" method="post">
                <div class="row mb-3">
                  <label for="inputText" class="col-sm-4 col-form-label">Select Category</label>
                  <div class="col-sm-8">
                    <select name="brandcat_name" id="categoryID" onchange="return getSubcatData()" class="form-control">
                        <option value="0">Please select Category</option>
                        <% for(var ct of categoryData){%>
                            <option value="<%= ct.id%>"><%= ct.category_name%></option>
                        <%}%>
                    </select>
                  </div>
                </div>
                <div class="row mb-3">
                  <label for="inputText" class="col-sm-4 col-form-label">Select Sub_Category</label>
                  <div class="col-sm-8">
                    <select name="brandSubCat_name" id="SubCatId" onchange="return getextraData()" class="form-control">
                        <option value="0">Please select Sub_Category</option>
                        <% for(var st of subCatData){%>
                            <option value="<%= st.id%>"><%= st.sub_category_name%></option>
                        <%}%>
                    </select>
                  </div>
                </div>
                <div class="row mb-3">
                  <label for="inputText" class="col-sm-4 col-form-label">Select Extra_Category</label>
                  <div class="col-sm-8">
                    <select name="brandExtra_name" id="extraId" class="form-control">
                        <option value="0">Please select Extra_Category</option>
                        <% for(var et of extraData){%>
                            <option value="<%= et.id%>"><%= et.extra_category_name%></option>
                        <%}%>
                    </select>
                  </div>
                </div>
                <div class="row mb-3">
                  <label for="inputText" class="col-sm-4 col-form-label">Enter Brand</label>
                  <div class="col-sm-8">
                    <input type="text" name="brand_name" placeholder="Enter Brand" class="form-control">
                  </div>
                </div>
                <div class="row mb-3">
                  <div class="col-sm-10">
                    <button type="submit" class="btn btn-primary">Add Brand</button>
                  </div>
                </div>
              </form>
            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->
<%- include("footer")%>